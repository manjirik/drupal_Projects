INSERT INTO `block_custom` VALUES ('15','<p>\r\n	<img alt="" src="/hartic/sites/default/files/322_364HandsWorld.png" style="height:322px; width:364px" /></p>\r\n','Center image (322x364)','full_html'),('16','<h2>\r\n	Where Trust Meets Innovation</h2>\r\n\r\n<p>\r\n	Democracy doesn’t stand still, and neither does technology. That’s why Hart has continued to innovate, &nbsp;for over 100 years.&nbsp; We deliver trusted election technology for those who matter most: voters, and the election officials who serve them.\r\n</br>\r\n <a href="/hartic/voting-solutions-hart">Learn more about this »</a></p>\r\n','Center right content','full_html'),('17','<div>\r\n	<h1>\r\n		Intro Headline for News</h1>\r\n\r\n	<p>\r\n		Additional introductory copy excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.</p>\r\n</div>\r\n','All News Header','full_html'),('18','<div>\r\n	<h1>\r\n		Intro Headline for News</h1>\r\n\r\n	<p>\r\n		Additional introductory copy excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.</p>\r\n</div>\r\n','All Press Release Header','full_html'),('19','<h1>\r\n	Corporate Headquarters</h1>\r\n','Contact Form Header1','full_html'),('20','<p>\r\n	Hart InterCivic<br />\r\n	15500 Wells Port Drive<br />\r\n	Austin, TX 78728<br />\r\n	512.252.6400<br />\r\n	800.223.HART<br />\r\n	FAX: 512.252.6466</p>\r\n','Contact Form Header2','full_html'),('21','<p>\r\n	Copyright © 2013 Hart InterCivic Inc. All rights reserved. <a href="../sites/default/files/eScan_brief.pdf">Terms of Use | Privacy Policy</a></p>\r\n','Footer details ','full_html'),('22','<p>\r\n	sdfasdf</p>\r\n','Description - is this the name','full_html'),('23','<p>\r\n	Copyright © 2013 Hart InterCivic Inc.</p>\r\n\r\n<p>\r\n	All rights reserved.</p>\r\n\r\n<p>\r\n	&nbsp;<a href="../sites/default/files/eScan_brief.pdf">Terms of Use | Privacy Policy</a></p>\r\n','Footer details for mobile','full_html');

INSERT INTO `block_custom` VALUES ('1','1-800-223-HART','1-800-223-HART','filtered_html'),('2','<p>\r\n	Hart InterCivic helps state and local governments conduct secure, accurate, and reliable elections.</p>\r\n\r\n<p class="hartcontact">\r\n	15500 Wells Port Drive, Austin, TX 78728</p>','ABOUT HART INTERCIVIC','full_html'),('3','<h3>	Connect with a Hart voting expert today. <span>1-800-223-HART</span></h3>\r\n','ConnectWith','filtered_html'),('4','<div>\r\n	<div>\r\n		<div>\r\n			<div>\r\n				<img src="http://50.56.201.199/hartic/sites/all/themes/Hartic_hero_image_1/images/Voting_cycle_graph.png" /></div>\r\n		</div>\r\n\r\n		<div>\r\n			<h2>\r\n				Homepage Subheadline</h2>\r\n\r\n			<p>\r\n				Lorem ipsum dolor sit amet, consectetur adipiscing elit. Sed vitae enim sed massa accumsan consequat nec vel erat. Nunc tortor enim, vehicula sit amet pulvinar non, aliquet sit amet lorem. Vivamus ac sem quis tortor pellentesque laoreet.<a href="#">Learn more about this »</a></p>\r\n		</div>\r\n	</div>\r\n</div>\r\n','Homepage Block','full_html'),('5','<p>\r\n	&nbsp;</p>\r\n\r\n<div>\r\n	<h3>\r\n		Connect with a Hart voting expert today.    <span>1-800-223-HART</span></h3>\r\n</div>\r\n\r\n<p>\r\n	&nbsp;</p>\r\n','Connect with without lines','full_html'),('7','<h4>\r\n	<span>Have a question? </span><span>Talk to an election expert today.</span><span> 1-800-223-HART</span></h4>\r\n','Expert Block','filtered_html'),('10','<h4>\r\n	Optional, Addtional Call-To-Action »</h4>\r\n','Optional Call ','full_html'),('12','<h1>\r\n	Intro Headline for News</h1>\r\n\r\n<p>\r\n	Additional introductory copy excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.</p>\r\n','News Header','full_html'),('13','<h1>\r\n	Intro Headline for Blog</h1>\r\n\r\n<p>\r\n	Additional introductory copy excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.</p>\r\n','Blog Header','full_html'),('14',
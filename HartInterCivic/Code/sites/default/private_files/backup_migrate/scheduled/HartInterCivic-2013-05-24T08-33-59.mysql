INSERT INTO `field_revision_field_cycle_detail` VALUES ('node','cycle_information','0','331','410','und','0','<p>\r\n	The Hart Voting System is a multi-faceted voting system supported by project management, training, service, and support. Along with extensive technical expertise, Hart has a clear understanding of the election process and the needs of election officials.</p>\r\n','','full_html'),('node','cycle_information','0','332','411','und','0','<p>\r\n	“Smart technology” is a design and development principle dedicated to building not just for today, but for tomorrow.&nbsp; ​</p>\r\n\r\n<p>\r\n	With just two main voting devices (paper and electronic), and a software suite that is configurable and scalable,</p>\r\n\r\n<p>\r\n	Verity can be implemented for many different needs and types of implementations.</p>\r\n','','full_html');

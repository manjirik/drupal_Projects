INSERT INTO `menu_custom` VALUES ('main-menu','Main menu','The <em>Main</em> menu is used on many sites to show the major sections of the site, often in a top navigation bar.'),('management','Management','The <em>Management</em> menu contains links for administrative tasks.'),('menu-2002-hart-voting-system','Hart Voting System:',''),('menu-2005-verity','Verity',''),('menu-about-hart-intercivic','ABOUT HART INTERCIVIC MENU','The ABOUT HART INTERCIVIC menu contains links for footer last column.'),('menu-about-hart-intercivic-mobil','ABOUT HART INTERCIVIC MOBILE MENU',''),('menu-blog-detail','Blog Detail',''),('menu-blog-menu','Blog',''),('menu-contact-form','Contact Us',''),('menu-election-resources','ELECTION RESOURCES','TheELECTION RESOURCES menu contains links for footer third column.'),('menu-election-resources-sec-menu','Election Resources',''),('menu-electronic-poll-book','Electronic Poll Book',''),('menu-mobile-main-menu','Mobile Main Menu','Main Menus for Mobile theme'),('menu-news','News & Events',''),('menu-news-detail','News Detail',''),('menu-press-release','Press Release',''),('menu-press-release-detail','Press Release Detail',''),('menu-robin-test','Robin Test',''),('menu-sales-support','SALES & SUPPORT','The SALES & SUPPORT menu contains links for footer second column.'),('menu-sample-menu','Sample Menu','This is a test menu for demonstrative purposes.'),('menu-view-all-products','VIEW ALL PRODUCTS','The VIEW ALL PRODUCTS menu contains links for footer first column.'),('menu-voting-solution-from-hart','Voting Solution from hart',''),('navigation','Navigation','The <em>Navigation</em> menu contains links intended for site visitors. Links are added to the <em>Navigation</em> menu automatically by some modules.'),('user-menu','User menu','The <em>User</em> menu contains links related to the user''s account, as well as the ''Log out'' link.');
